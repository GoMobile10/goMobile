<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/naviInfo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#CCFFFFFF"
        android:visibility="invisible">

    <ImageView
        android:id="@+id/pickuplistDOWN"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/pickuplistUP"
        android:layout_below="@+id/pickuplistLEFT"
        android:layout_marginTop="24dp"
        android:src="@drawable/scanned" />

    <ImageView
        android:id="@+id/pickuplistUP"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="26dp"
        android:src="@drawable/scanned" />

    <ImageView
        android:id="@+id/pickuplistLEFT"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="40dp"
        android:src="@drawable/scanned" />

    <ImageView
        android:id="@+id/pickuplistRIGHT"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_above="@+id/pickuplistDOWN"
        android:layout_alignParentRight="true"
        android:layout_marginRight="45dp"
        android:src="@drawable/scanned" />


</RelativeLayout>